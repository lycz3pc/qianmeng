<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xhpower.qianmeng.dao.CategoryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xhpower.qianmeng.entity.Category">
        <id column="id" property="id" />
        <result column="category_name" property="categoryName" />
        <result column="category_column" property="categoryColumn" />
        <result column="sort" property="sort" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, category_name AS categoryName, category_column AS categoryColumn, sort, status
    </sql>

</mapper>
